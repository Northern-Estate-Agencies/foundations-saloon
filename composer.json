{
    "name": "northern-estate-agencies/foundations-saloon",
    "type": "library",
    "require": {
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.0",
        "fakerphp/faker": "^1.22",
        "guzzlehttp/psr7": "^2.5",
        "guzzlehttp/guzzle": "^7.7",
        "saloonphp/saloon": "^3.0",
        "saloonphp/pagination-plugin": "^2.0"
    },
    "require-dev": {
        "laravel/sanctum": "^3.0",
        "laravel/pint": "^1.6",
        "pestphp/pest": "^2.6",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "FoundationsSaloon\\": "src/FoundationsSaloon/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
